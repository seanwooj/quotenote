      .social
        - {:twitter => 'https://twitter.com/QUOTENOTE_it', :facebook => 'https://www.facebook.com/pages/QuoteNote/575718842568593', :instagram => 'https://instagram.com/quotenote_it/', :tumblr => 'http://quotenote-it.tumblr.com/ '}.each do |social_name, url|
          %a{:href => url, :target => '_blank'}
            %i{:class => "fi-social-#{social_name.to_s}"}
